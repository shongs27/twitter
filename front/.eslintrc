{
  //babel eslint를 사용함으로써 최신문법도 에러를 발생시키지 않는다
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
    },
    "extends": [
      //약한 recomended들
      // "eslint:recommended",
      // "plugin:react/recommended"

      //강한 airbnb
      "airbnb"
    ],
    "plugins": [
      "import",
      "react-hooks"
    ],
    "rules": {
      // "jsx-a11y/label-has-associated-control": "off",
      // "jsx-a11y/anchor-is-valid": "off",
      // "no-console": "off",
      // "no-underscore-dangle": "off",
      // "react/forbid-prop-types": "off",
      // "react/jsx-filename-extension": "off",
      // "react/jsx-one-expression-per-line": "off",
      // "object-curly-newline": "off",
      // "linebreak-style": "off",
      // "no-param-reassign": "off"
    }
  }